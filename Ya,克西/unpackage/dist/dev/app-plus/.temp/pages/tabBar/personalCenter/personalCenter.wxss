
page {
  height: 100%;
  background: rgb(255, 251, 255);
}
.status_bar {
  height: var(--status-bar-height);
  width: 100%;
}
.top_view {
  height: var(--status-bar-height);
  width: 100%;
  position: fixed;
  top: 0;
}
.PopupBox {
  width: 500rpx;
  height: 350rpx;
  position: fixed;
  left: 300rpx;
  z-index: 555;
}
.bg_img {
  position: fixed;
  width: 100%;
  height: 500rpx;
  -webkit-filter: blur(1px);
  filter: blur(1px);
}
.bgSrc {
  width: 100%;
  height: 100%;
  border-radius: 3rpx;
}
.user {
  position: fixed;
  top: 120rpx;
  margin: 10rpx 0 0 20rpx;
  width: 95%;
  height: 340rpx;
}
.user1 {
  display: none;
}
.loginText {
  position: fixed;
  top: 200rpx;
  left: 50rpx;
  font-size: 50rpx;
  color: #0c2647;
}
.loginTextImg {
  border: 2px solid white;
  width: 140rpx;
  height: 140rpx;
  border-radius: 50%;
}
.loginText1 {
  display: none;
}
.userImg {
  position: fixed;
  top: 180rpx;
  left: 60rpx;
  width: 140rpx;
  height: 140rpx;
  border-radius: 50%;
  display: block;
  margin: 0 auto;
  border: 3px solid white;
}
.userNameBox {
  position: fixed;
  top: 340rpx;
  left: 20rpx;
  text-align: center;
  width: 240rpx;
  height: 60rpx;
  font-size: 35rpx;
  color: white;
}
.ID {
  position: fixed;
  top: 400rpx;
  left: 50rpx;
  width: 190rpx;
  height: 40rpx;
  line-height: 40rpx;
  padding-left: 20rpx;
  border-radius: 50rpx;
  background: rgba(0, 0, 0, 0.2);
  font-size: 25rpx;
  color: white;
}
.IDnumber {
  position: fixed;
  top: 400rpx;
  left: 110rpx;
  font-size: 30rpx;
}
.sig {
  position: fixed;
  top: 440rpx;
  left: 70rpx;
  color: white;
  font-size: 30rpx;
  font-weight: 400;
}
.function {
  margin: 0 auto;
  width: 100%;
  height: 900rpx;
  position: fixed;
  top: 500rpx;
  border-top: 14px solid rgb(239,239,247);
}
._li {
  list-style: none;
  margin: 0 auto;
  height: 100rpx;
  line-height: 90rpx;
  font-size: 35rpx;
  padding-left: 90rpx;
  background: #ffffff;
  margin-bottom: 5rpx;
  border-bottom: 2px solid rgb(239,239,247);
}
.iconImg {
  width: 35rpx;
  height: 35rpx;
  padding-right: 20rpx;
  padding-top: 30rpx;
}
.functionText {
  position: relative;
  top: -2rpx;
  color: rgb(999);
}
.arrows {
  width: 30rpx;
  height: 40rpx;
  margin-top: 30rpx;
  position: fixed;
  left: 600rpx;
}
.reg,
.login {
  font-size: 50rpx;
}
.updateImg {
  background: white;
  width: 420rpx;
  height: 60rpx;
  border-bottom: 1px solid rgb(239,239,247);
  padding-bottom: 20rpx;
  margin-bottom: 20rpx;
  font-size: 30rpx;
}
.quxiaoClick {
  background: white;
  width: 420rpx;
  height: 60rpx;
  font-size: 30rpx;
}
.uni-mask {
  position: fixed;
  z-index: 998;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background-color: rgba(0, 0, 0, .3);
}
.uni-popup {
  position: absolute;
  z-index: 999;
  background-color: #ffffff;
  box-shadow: 0 0 30rpx rgba(0, 0, 0, .1);
}
.uni-popup-middle {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  width: 420rpx;
  height: 120rpx;
  border-radius: 10rpx;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  padding: 30rpx;
}
.uni-popup-top {
  top: 0;
  left: 0;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
}
.uni-popup-bottom {
  left: 0;
  bottom: 0;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
}